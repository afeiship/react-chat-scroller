{
  "presets": [
    "react-native"
  ],
  "plugins": [
    [
      "module-alias",
      [
        { "src": "./src", "expose": "src" },
        { "src": "./src/components", "expose": "components" },
        { "src": "./src/components/views", "expose": "views" },
        { "src": "./src/components/services", "expose": "services" },
        { "src": "./src/components/interceptors", "expose": "interceptors" }
      ]
    ]
  ]
}
